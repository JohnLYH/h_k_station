<?xml version="1.0" encoding="UTF-8" ?>
<templates xml:space="preserve">
	<!--x詳情按钮和删除按鈕-->
    <t t-name="inventory_tree_operation">
        <div class="layui-btn-group">
            <button class="layui-btn layui-btn-sm" type="object"  name="inventory_edit"
            >詳情</button>
        </div>
    </t>

    <t t-name="inventory_search_tree">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-2 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">庫存編碼:</span>
                    <span for="inventory_id" class="col-xs-8"></span>
                </div>
                <div class="col-lg-2 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">描述:</span>
                    <span for="inventory_description" class="col-xs-8"></span>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">最後更新時間:</span>
                    <span for="lastest_update_time" class="col-xs-8" ></span>
                </div>
                <div class="col-md-4 col-sm-12 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <button type="button" class="btn btn-default search_extend_apply">
                        搜索
                    </button>
                    <button type="button" class="btn btn-default search_extend_reset">
                        重置
                    </button>
                </div>
            </div>
        </div>
    </t>
</templates>
