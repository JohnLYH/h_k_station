<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <data>
        <record id="import_date" model="ir.ui.view">
            <field name="name">導入人員信息</field>
            <field name="model">user.import_date</field>
            <field name="arch" type="xml">
                <form creata="0" edit="0" class="noOpen">
                    <sheet>
                        <group>
                            <field name="file"></field>
                        </group>
                        <footer>
                            <button name='import_record'
                                    string='導入人員信息'
                                    class='btn-primary'
                                    type='object'/>
                            <button string='取消'
                                    class='btn-default'
                                    special='cancel'/>
                        </footer>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>