<?xml version="1.0" encoding="utf-8" ?>
<template>
    <!-- plan搜索 -->
    <t t-name="plan_search_template">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">工單編號:</span>
                    <span for="num" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">設備編號:</span>
                    <span for="equipment_num" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">計劃時間:</span>
                    <span for="plan_start_time" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <button type="button" class="btn btn-primary search_extend_apply">
                        搜索
                    </button>
                    <button type="button" class="btn btn-default search_extend_reset">
                        重置
                    </button>
                    <button type="button" class="btn layui-btn-sm btn-primary put_in_excel">
                        導入
                    </button>
                    <form style="display:none" action="">
                        <input type="file" name="file" value=""/>
                    </form>
                </div>
            </div>
        </div>
    </t>

    <!-- 工單搜索 -->
    <t t-name="work_order_search_template">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">工單編號:</span>
                    <span for="num" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">設備編號:</span>
                    <span for="equipment_num" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">狀態:</span>
                    <span for="status" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">計劃時間:</span>
                    <span for="plan_start_time" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">實際開始時間:</span>
                    <span for="actual_start_time" class="col-xs-8"/>
