<?xml version="1.0" encoding="utf-8" ?>
<template>
    <!-- plan搜索 -->
    <t t-name="plan_search_template">
        <div class="form-inline custom_search_box" search_pannel_js_class="plan_search_pannel">
            <div class="container-fluid">
                <div class="col-md-3 col-sm-6 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">工單編號:</span>
                    <div class="col-md-8 col-sm-12 col-xs-12">
                        <div for="num" class="form-widget-item"/>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">設備編號:</span>
                    <div class="col-md-8 col-sm-12 col-xs-12">
                        <div for="equipment_num" class="form-widget-item"/>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;">計劃時間:</span>
                    <div class="col-md-8 col-sm-12 col-xs-12">
                        <div for="plan_start_time" class="form-widget-item"/>
                    </div>
                </div>

                <div class="col-md-3 col-sm-6 col-xs-12" style="margin-bottom:5px; height:40px; line-height:40px;">
                    <div class="form-widget-item col-md-12 col-sm-12 col-xs-12">
                        <button type="button" class="btn btn-primary search_extend_apply">
                            搜索
                        </button>
                        <button type="button" class="btn btn-default search_extend_reset">
                            重置
                        </button>
                        <button type="button" class="btn layui-btn-sm btn-primary put_in_excel">
                            導入
                        </button>
                        <form style="display:none" action="">
                            <input type="file" name="file" value=""/>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </t>

    <!-- 工單搜索 -->
