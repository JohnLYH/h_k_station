                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">實際結束時間:</span>
                    <span for="actual_end_time" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <button type="button" class="btn btn-primary search_extend_apply">
                        搜索
                    </button>
                    <button type="button" class="btn btn-default search_extend_reset">
                        重置
                    </button>
                    <button type="button" class="btn layui-btn-sm btn-primary export_excel">
                        導出
                    </button>
                </div>
            </div>
        </div>
    </t>

    <!--設備搜索-->
    <t t-name="equipment_search_template">
        <div class="form-inline custom_search_box">
            <div class="container-fluid">
                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">設備描述:</span>
                    <span for="description" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">設備編號:</span>
                    <span for="num" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">設備序列號:</span>
                    <span for="serial_number" class="col-xs-8"/>
                </div>

                <div class="col-lg-3" style="height:40px; line-height:40px;">
                    <span class="search_label" style="float:left;width:auto;">註冊時間:</span>
                    <span for="create_date" class="col-xs-8"/>
                </div>

                <div class="col-lg-12" style="height:40px; line-height:40px;">
                    <button type="button" class="btn btn-primary equipment_search_apply">
                        搜索
                    </button>
                    <button type="button" class="btn btn-default equipment_reset_search">
                        重置
                    </button>
                    <button type="button" class="btn btn-warning put_in_equipment">
                        導入設備
                    </button>
                    <form style="display:none" action="">
                        <input type="file" name="file" value=""/>
                    </form>
                    <button type="button" class="btn btn-warning export_qr_code">
                        導出二維碼
                    </button>
                </div>
            </div>
        </div>
    </t>

